		"element-ui": "2.13.0",
		"html2canvas": "^1.0.0-rc.7",
		"js-cookie": "2.2.0",
		"js-md5": "^0.7.3",
		"normalize.css": "7.0.0",
		"nprogress": "0.2.0",
		"path-to-regexp": "2.4.0",
		"videojs-contrib-hls": "^5.15.0",
		"videojs-flash": "^2.2.1",
		"vue": "2.6.10",
		"vue-cropper": "^0.5.6",
		"vue-qr": "^2.2.1",
		"vue-router": "3.0.6",
		"vue-video-player": "^5.0.2",
		"vue2-frappe": "^1.2.3",
