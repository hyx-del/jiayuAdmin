		"@vue/cli-plugin-eslint": "^3.9.1",
		"@vue/cli-plugin-unit-jest": "3.6.3",
		"@vue/cli-service": "4.5.13",
		"@vue/test-utils": "1.0.0-beta.29",
		"autoprefixer": "^9.5.1",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "10.0.1",
		"babel-jest": "23.6.0",
		"chalk": "2.4.2",
